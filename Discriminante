#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"

using namespace std;

void main(){
	float b,a,c,x;
	cout<<"Ingrese el valor de b: ";
	cin>>b;
	cout<<"Ingrese el valor de a: ";
	cin>>a;
	cout<<"Ingrese el valor de c: ";
	cin>>c;
	x=pow(b,2)-4*a*c;
	cout<<"El Discriminante es: "<<x;
	cout<<endl;
	getch();
}
